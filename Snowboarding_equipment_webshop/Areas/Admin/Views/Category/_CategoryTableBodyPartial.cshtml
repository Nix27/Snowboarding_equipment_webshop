@model IEnumerable<CategoryVM>

@foreach (var category in Model)
{
    <tr>
        <td>@category.Name</td>
        <td class="text-end">
            <div class="w-25 btn-group" role="group">
                <a asp-action="UpdateCategory" asp-route-id="@category.Id" class="primary-btn mx-2 px-5 py-2">
                    <i class="bi bi-pencil"></i>
                </a>
                <a onclick="deleteWarning('/Admin/Category/DeleteCategory/'+@category.Id, ' #category-table')" class="btn btn-danger mx-2">
                    <i class="bi bi-trash3"></i>
                </a>
            </div>
        </td>
    </tr>
}
